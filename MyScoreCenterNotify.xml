<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
    <id>42010</id>
    <defaultcontrol>4</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <controls>
        <control>
            <description>group element</description>
            <type>group</type>
            <animation effect="slide" start="0,150" time="500">WindowOpen</animation>
            <animation effect="fade" start="100" end="0" time="1000">WindowClose</animation>
            <control>
                <id>0</id>
                <type>image</type>
                <visible>yes</visible>
                <posX>360</posX>
                <posY>790</posY>
                <width>1200</width>
                <height>307</height>
                <texture>dialog_background.png</texture>
            </control>
            <control>
                <id>3</id>
                <description>Heading text label</description>
                <type>label</type>
                <posX>459</posX>
                <posY>834</posY>
                <font>font10</font>
                <textcolor>ffffffff</textcolor>
                <label>181</label>
            </control>
            <control>
                <id>4</id>
                <description>OK button</description>
                <type>button</type>
                <onleft>4</onleft>
                <onright>4</onright>
                <onup>4</onup>
                <ondown>4</ondown>
                <posX>1390</posX>
                <posY>822</posY>
                <width>75</width>
                <height>45</height>
                <textureFocus>dialogue_ok_focus.png</textureFocus>
                <textureNoFocus>dialogue_ok_nofocus.png</textureNoFocus>
                <label>-</label>
            </control>
            <control>
                <id>5</id>
                <description>logo</description>
                <type>image</type>
                <colordiffuse>ffffffff</colordiffuse>
                <posX>447</posX>
                <posY>880</posY>
                <width>72</width>
                <height>72</height>
                <texture>#thumb</texture>
                <colorkey>FFFF00FF</colorkey>
                <keepaspectratio>yes</keepaspectratio>
            </control>
            <control>
                <id>6</id>
                <description>textarea</description>
                <type>textbox</type>
                <onleft>4</onleft>
                <onright>4</onright>
                <onup>4</onup>
                <ondown>4</ondown>
                <posX>526</posX>
                <posY>879</posY>
                <width>945</width>
                <height>177</height>
                <spinPosX>1320</spinPosX>
                <spinPosY>1020</spinPosY>
                <label>-</label>
            </control>
        </control>
    </controls>
</window>
